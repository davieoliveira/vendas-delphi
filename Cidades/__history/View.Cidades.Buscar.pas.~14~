{$R *.dfm}

uses Model.Cidade.DM;

{ TViewHerancasBuscar1 }

procedure TViewbuscarCidade1.BuscarDados;
var LCondicao: string;
begin
  Lcondicao := '';
  case RdGroupFiltros.ItemIndex of
    0: LCondicao := '';
    1: LCondicao := '';
    2: LCondicao := '';
  end;

  inherited;
end;

